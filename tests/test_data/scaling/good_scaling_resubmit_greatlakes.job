#!/bin/bash
#SBATCH --job-name=resubmit
#SBATCH -t 5:00
#SBATCH -p standard
#SBATCH -N 1 --tasks-per-node=1
set echo
set -x

cd $SLURM_SUBMIT_DIR
## Your commands here:
files="tests/test_data/scaling/scaling_1 tests/test_data/scaling/scaling_36"
basename=tests/test_data/scaling/scaling

for file in ${files[@]}
do
    if [ ! -f ${file}.log ]
    then
        echo "${file}.log does not exist"
        sbatch --begin=now+10minutes ${basename}_resubmit.job
        exit 1
    fi
done
sbatch --begin=now+10minutes ${basename}_analysis.job
